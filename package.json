{
  "name": "git-branch-todo",
  "displayName": "Git Branch TODO",
  "description": "Modern branch-specific TODO lists stored in Git branch descriptions with drag & drop, inline editing, and smart error handling",
  "version": "1.0.0",
  "publisher": "digitalzen-app",
  "author": "digitalzen-app",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digitalzen-app/git-todo.git"
  },
  "bugs": {
    "url": "https://github.com/digitalzen-app/git-todo/issues"
  },
  "homepage": "https://github.com/digitalzen-app/git-todo#readme",
  "keywords": [
    "git",
    "todo",
    "task",
    "branch",
    "productivity",
    "project-management",
    "vscode-extension"
  ],
  "categories": [
    "Other",
    "SCM Providers"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "activationEvents": [
    "onCommand:gitTodo.openPanel"
  ],
  "contributes": {
    "commands": [
      {
        "command": "gitTodo.openPanel",
        "title": "Git Branch TODO: Open Panel",
        "category": "Git TODO",
        "icon": "$(checklist)"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "gitTodo.openPanel",
          "when": "workspaceFolderCount > 0"
        }
      ]
    }
  },
  "main": "./out/extension.js",
  "scripts": {
    "package": "mkdir -p dist && vsce package -o ./dist/git-branch-todo.vsix",
    "publish": "vsce publish",
    "install-local": "vsce install ./dist/git-branch-todo.vsix",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@types/vscode": "^1.74.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@vscode/vsce": "^3.6.0",
    "acorn": "^8.15.0",
    "acorn-jsx": "^5.3.2",
